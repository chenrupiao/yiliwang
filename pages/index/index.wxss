/**index.wxss**/
page{background: #f2f2f2;}
.page-body{width:100%;}
swiper{width:100%;}
.slide-image{width:100%;}
.container{position: relative;font-size: 0}
.search{position: fixed;width: 100%;top:0;z-index: 9;transition: 300ms;padding-bottom: 5px;}
.search .search-fl{width: 55rpx;float: left;text-align: center;margin-left: 24rpx;padding-top: 15rpx;}
.search .search-fl image{width: 35rpx;height: 35rpx;float: left;margin-left: 9rpx;}
.search .search-fl text{font-size: 18rpx;color: #fff;float: left;margin-top: 3rpx;}
.search .search-inp{width: 540rpx;height: 62rpx;float: left;position: relative;margin-left: 25rpx;margin-top: 10rpx;}
.search .search-inp image{width: 30rpx;height: 31rpx;position: absolute;top: 16rpx;left: 15rpx;}
.search .search-inp input{width: 540rpx;height: 62rpx;font-size: 30rpx;margin: 0 auto;background: rgba(255,255,255,1);border-radius: 8rpx;box-sizing: border-box;padding-left: 60rpx;}
.search .search-fr{float: left;text-align: center;margin-left: 15rpx;margin-top: 20rpx;}
.search .search-fr image{width: 16rpx;height: 10rpx;float: left;margin-top: 16rpx;margin-left: 4rpx;}
.search .search-fr text{font-size: 28rpx;color: #fff;float: left;}

.search2{position: fixed;width: 100%;top:0;background: #cc0011;z-index: 9;transition: 300ms;padding-bottom: 5px;}
.search2 .search-fl{width: 55rpx;float: left;text-align: center;margin-left: 24rpx;padding-top: 15rpx;}
.search2 .search-fl image{width: 35rpx;height: 35rpx;float: left;margin-left: 9rpx;}
.search2 .search-fl text{font-size: 18rpx;color: #fff;float: left;margin-top: 3rpx;}
.search2 .search-inp{width: 540rpx;height: 62rpx;float: left;position: relative;margin-left: 25rpx;margin-top: 10rpx;}
.search2 .search-inp image{width: 30rpx;height: 31rpx;position: absolute;top: 16rpx;left: 15rpx;}
.search2 .search-inp input{width: 540rpx;height: 62rpx;font-size: 30rpx;margin: 0 auto;background: rgba(255,255,255,1);border-radius: 8rpx;box-sizing: border-box;padding-left: 60rpx;}
.search2 .search-fr{float: left;text-align: center;margin-left: 15rpx;margin-top: 20rpx;}
.search2 .search-fr image{width: 16rpx;height: 10rpx;float: left;margin-top: 16rpx;margin-left: 4rpx;}
.search2 .search-fr text{font-size: 28rpx;color: #fff;float: left;}

.icon{background: #fff;width: 100%;display: inline-block;border-bottom: 1px solid #ccc;}
.icon navigator{float: left;width: 25%;text-align: center;padding: 40rpx 0}
.icon navigator image{width: 84rpx;height: 86rpx;}
.icon navigator text{display: block;font-size: 26rpx;color: #333;padding-top: 15rpx;}

.toutiao{background: #fff;width: 100%;display: inline-block;box-sizing: border-box;padding: 30rpx 24rpx;}
.toutiao-txt{float: left;font-size: 28rpx;color: #999;line-height: 34rpx;}
.toutiao-txt view{font-size: 30rpx;color: #333;font-weight: bold;line-height: 34rpx;float: left}
.toutiao-txt text{width: 75rpx;height: 34rpx;line-height: 34rpx;text-align: center;display: block;background: #cc0011;border-radius: 3px;font-size: 28rpx;color: #fff;font-weight: bold;float: left;margin-right: 8rpx;}
.toutiao navigator:first-of-type{font-size: 28rpx;color: #333;line-height: 34rpx;float: left;margin-left: 8rpx;}
.toutiao navigator:last-of-type{font-size: 28rpx;color: #333;line-height: 34rpx;float: right;padding: 0 10rpx;border-left: 1px solid #ccc}

.index-list1{margin-top: 20rpx;background: #fff;width: 100%;display: inline-block;border-bottom: 1px solid #e6e6e6;}
.bt{font-size: 30rpx;color: #333;width: 100%;text-align: center;padding: 20rpx 0 40rpx 0;border-bottom: 1px solid #e6e6e6;background: #fff}
.bt navigator:first-of-type{padding: 0 14rpx;display: inline-block;background: #fff;}
.bt view{height: 1px;width: 300rpx;background: #ccc;margin: 0 auto;margin-top: -20rpx;}
.bt navigator.super{float: right;margin-right: 24rpx;margin-top: -20rpx;font-size: 26rpx;color: #999;}
.bt navigator image{width: 22rpx;height: 22rpx;margin-left: 5rpx;}
.index-list1-content{background: #fff;width: 100%;display: inline-block;box-sizing: border-box;padding: 0 24rpx;padding-top: 10rpx;}
.index-list1-content navigator{float: left;width: 33.3%;text-align: center;}
.index-list1-content navigator image{width: 218rpx;height: 218rpx;}
.index-list1-content navigator text{display: block}
.index-list1-content navigator text.super{font-size: 26rpx;color: #666;padding: 25rpx 0;}
.index-list1-content navigator text:last-of-type{font-size: 26rpx;color: #333;padding-bottom: 25rpx;}
.index-list1-content navigator view{background: #cc0011;width: 150rpx;height: 40rpx;margin: 0 auto;border-radius: 8px 8px 0 0;font-size: 22rpx;color: #fff;line-height: 40rpx;margin-top: 20rpx;margin-bottom: 23rpx;}
.index-list1-content navigator view image{width: 20rpx;height: 20rpx;float: left;margin-top: 10rpx;margin-left: 15rpx;}

.index-list2{background: #fff;display: inline-block;margin-top: 20rpx;width: 100%;border-bottom: 1px solid #e6e6e6;}
.index-list3{background: #fff;display: inline-block;width: 100%;border-bottom: 1px solid #e6e6e6;}
.bt2{width: 100%;border-bottom: 1px solid #e6e6e6;padding: 16rpx 0;display: inline-block;}
.bt2-fl{font-size: 30rpx;color: #333;padding-left: 20rpx;line-height: 40rpx;border-left: 6rpx solid #cc0011;float: left;}
.bt2-fr{font-size: 26rpx;color: #999;float: right;margin-right: 24rpx;line-height: 40rpx;}
.bt2-fr image{width: 20rpx;height: 20rpx;margin-left: 5rpx;}
.index-list2-content{width: 100%;box-sizing: border-box;padding: 0 24rpx;}
.list2-content-fl{float: left;position: relative;margin: 20rpx 0}
.list2-content-fl image{width: 340rpx;height: 340rpx;}
.list2-content-txt{position: absolute;bottom: 40rpx;left: 50%;margin-left: -100rpx;text-align: center;background: rgba(255,255,255,0.8);width: 200rpx} 
.list2-content-txt text:first-of-type{color: #333;font-size: 24rpx;padding: 10rpx 0;display: block;}
.list2-content-txt text:last-of-type{color: #333;font-size: 28rpx;padding-bottom: 10rpx;display: block}
.list2-content-fr{float: right;width: 350rpx;margin-top: 20rpx;}
.list2-content-fr navigator{float: left;width: 350rpx;padding: 10rpx 0;}
.list2-content-txt2{float: left;width: 185rpx;}
.list2-content-txt2 text:first-of-type{font-size: 26rpx;color: #333;line-height: 39rpx;display: block;padding-bottom: 15rpx;}
.list2-content-txt2 text:last-of-type{color: #cc0011;font-size: 30rpx;display: block;}
.list2-content-fr image{float: left;width: 140rpx;height: 140rpx;}
.xuxian{width: 350rpx;float: left;border-bottom: 1px dashed #ccc;margin: 10rpx 0;}
.xuxian:last-of-type{display: none}

.banner image{width: 100%;}

.index-list4{background: #fff;display: inline-block;margin-top: 20rpx;width: 100%;border-bottom: 1px solid #e6e6e6;}
.index-list4-content{width: 100%;box-sizing: border-box;padding: 0 24rpx;margin-bottom: 20rpx;display: inline-block;}
.index-list4-content navigator{margin-top: 20rpx;position: relative;}
.index-list4-content navigator:nth-of-type(2n+1){float: left;}
.index-list4-content navigator:nth-of-type(2n+2){float: right;}
.index-list4-content navigator image{width: 340rpx;height: 160rpx;}
.index-list4-content navigator text{font-size: 28rpx;color: #333;width: 180rpx;height: 70rpx;line-height: 70rpx;text-align: center;position: absolute;top: 50%;margin-top: -35rpx;left: 50%;margin-left: -90rpx;background: rgba(255,255,255,0.8);}

.index-list5{margin-top: 20rpx;width: 100%;display: inline-block;border-bottom: 1px solid #e6e6e6;}
.index-list5-content{width: 100%;box-sizing: border-box;padding: 0 24rpx;display: inline-block;}
.index-list5-content navigator{width: 340rpx;background: #fff;margin-top: 20rpx;}
.index-list5-content navigator:nth-of-type(2n+1){float: left;}
.index-list5-content navigator:nth-of-type(2n+2){float: right;}
.index-list5-content navigator image{width: 340rpx;height: 340rpx;}
.index-list5-content navigator text{display: block;box-sizing: border-box;width: 100%;padding-left: 10rpx;}
.index-list5-content navigator text:first-of-type{font-size: 26rpx;color: #333;line-height: 41rpx;}
.index-list5-content navigator text:last-of-type{font-size: 32rpx;color: #cc0011;padding-bottom: 20rpx;}