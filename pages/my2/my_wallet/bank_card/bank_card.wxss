/* pages/my2/my_wallet/bank_card/bank_card.wxss */
.bank-content{padding: 0 24rpx;box-sizing: border-box;width: 100%;}
.bank-list1 navigator{width: 100%;background: #f02387;margin-top: 20rpx;height: 160rpx;border-radius: 5px;}
.bank-list1 navigator image{width: 67rpx;height: 67rpx;float: left;margin-top: 46rpx;margin-left: 42rpx;}
.bank-txt{margin-top:45rpx;float: left;margin-left: 15rpx}
.bank-txt text{font-size: 28rpx;color: #fff;display: block;}
.bank-txt text:nth-of-type(2){padding-top: 6rpx;}
.bank-super{color: #fff;font-size: 28rpx;float: right;margin-top: 45rpx;margin-right: 20rpx;}

.bank-btn navigator{width: 100%;background: #fff;color: #666;font-size: 30rpx;text-align:center;height: 180rpx;line-height: 180rpx;margin-top: 20rpx;border-radius: 5px;}