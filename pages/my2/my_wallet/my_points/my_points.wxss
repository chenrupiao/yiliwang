/* pages/my2/my_wallet/my_points/my_points.wxss */
.points-top{width: 100%;padding: 0 24rpx;box-sizing: border-box;padding-bottom: 50rpx;background: #fff;}
.points-top text:first-of-type{font-size: 24rpx;color: #999;padding-left: 35rpx;display: block;padding-top: 55rpx;}
.points-top text:last-of-type{font-size: 100rpx;color: #cc0011;padding-left: 35rpx;display: block;padding-top: 15rpx;}

.points-bottom{width: 100%;box-sizing: border-box;padding: 0 24rpx;background: #fff;margin-top: 20rpx;}
.points-bottom navigator{width: 100%;display: inline-block;border-bottom: 1px solid #e6e6e6;}
.points-bottom navigator:last-of-type{border: none;}
.points-bottom navigator .super{font-size: 46rpx;color: #cc0011;float: right;height: 120rpx;line-height: 120rpx;}
.points-txt{float: left;}
.points-txt text:first-of-type{font-size: 30rpx;color: #333;display: block;padding-top: 25rpx}
.points-txt text:last-of-type{font-size: 24rpx;color: #666;display: block;padding-top: 15rpx;}
.points-bt{width: 100%;height: 90rpx;line-height: 90rpx;border-bottom: 1px solid #e6e6e6;}
.points-bt text{font-size: 30rpx;color: #333;}