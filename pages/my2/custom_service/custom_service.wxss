/* pages/my2/custom_service/custom_service.wxss */
.kf-list1{width: 100%;margin-top: 20rpx;background: #fff;}
.kf-bt{width: 100%;height: 80rpx;line-height: 80rpx;box-sizing: border-box;padding: 0 24rpx;background: #f2f2f2;}
.kf-bt text{color: #666;font-size: 28rpx;}

.kf-btn{width: 100%}
.kf-btn navigator{width: 25%;text-align: center;background: #fff;padding: 45rpx 0;float: left;}
.kf-btn navigator image{width: 50rpx;height: 48rpx;}
.kf-btn navigator text{font-size: 28rpx;color: #333;padding-top: 20rpx;display: block;}

.container {align-items:stretch;}
.navbar{flex: none;display: flex;background: #fff;width: 100%;border:1px solid #545e6e;border-radius: 3px;}  
.navbar .item{position: relative;line-height: 68rpx;font-size:28rpx;color: #545e6e;width: 33.3%;border-right: 1px solid #545e6e;box-sizing: border-box;text-align: center;}
.navbar .item:last-of-type{border: none;}  
.navbar .item.active{color: #fff;background: #545e6e;}   

.child{display: none;width: 100%;box-sizing: border-box;padding: 0 24rpx;}
.child.active{display: block;}
.child navigator{width: 100%;height: 100rpx;border-bottom: 1px solid #e6e6e6;}
.child navigator image{width: 15rpx;height: 27rpx;margin-top: 36rpx;float: right;}
.child navigator text{font-size: 30rpx;color: #333;line-height: 100rpx;float: left;}
.child navigator text:nth-of-type(2){float: right;}

.kf-btn2{width: 100%;box-sizing: border-box;padding: 30rpx 24rpx;border-bottom:1px solid #e6e6e6;}





